<form class="" action="/articles/<%= @article.id %>" method="post">
<ul>
  <li><label for="title">Title</label>
  <input type="text"  name="title" id="title" value="<%= @article.title %>"></li>

  <li><label for="content">Content</label>
  <input type="text" name="content" id="content" value="<%= @article.content %>"></li>
</ul>
<input id="hidden" type="hidden" name="_method" value="PATCH">
  <input type="submit" value="Submit">

  </form>
